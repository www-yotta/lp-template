{
  "name": "test-lp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "autoprefixer": "^10.3.4",
    "browser-sync": "^2.27.5",
    "cpx": "^1.5.0",
    "cssnano": "^5.0.8",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "uglify-js": "^3.14.2",
    "watch": "^1.0.2"
  },
  "dependencies": {},
  "scripts": {
    "css:sass": "node-sass src/sass/style.scss -o dist/css --output-style expanded --source-map dist/css",
    "css/postcss": "postcss dist/css/style.css -o dist/css/style.min.css",
    "css": "npm-run-all -s css:*",
    "js/uglifyjs": "uglifyjs dist/js/index.js -o dist/js/index.min.js",
    "js:babel": "babel ./src/js --out-dir ./dist/js",
    "js": "npm-run-all -s js:*",
    "html": "cpx -C ./src/html/*html ./dist",
    "watch:server": "browser-sync start --server dist --files 'dist, **/*.html, !node_modules/**/*'",
    "watch:css": "watch \"npm run css\" ./src/sass",
    "watch:js": "watch \"npm run js\" ./src/js",
    "watch:html": "watch \"npm run html\" ./src/html",
    "watch": "npm-run-all -p watch:*"
  },
  "browserslist": [
    ">0.25% in JP",
    "not ie <= 10",
    "not op_mini all"
  ]
}
